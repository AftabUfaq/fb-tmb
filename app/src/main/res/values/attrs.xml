<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="HorizontalPicker">
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="values" format="reference" />
        <attr name="android:ellipsize" />
        <attr name="android:marqueeRepeatLimit" />
        <attr name="dividerSize" format="dimension" />
        <attr name="sideItems" format="integer" />
        <attr name="karthikPugalMeasure" format="integer" />
    </declare-styleable>


    <attr name="horizontalPickerStyle" format="reference" />

    <declare-styleable name="SeekBarHint">

        <attr name="popupWidth" format="dimension" />
        <attr name="yOffset" format="dimension" />
        <attr name="popupStyle" format="enum">
            <enum name="follow" value="0"/>
            <enum name="fixed" value="1"/>
        </attr>

    </declare-styleable>

</resources>